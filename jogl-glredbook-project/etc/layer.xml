<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
#set( $examples= $ant.examples.split("[ ,]+"))
<filesystem>
	<folder name="Templates">
		<folder name="Project">
			<folder name="OpenGL">
				<folder name="Redbook Demos">
					#foreach( $example in $examples )
					
					<file name="${example}.zip" url="nbresloc:/net/highteq/gamedev/nbm/joglproject/resources/jogl-project.zip">
						<attr name="demo" stringvalue="glredbook10.$example"/>
						<attr name="projectPackage" stringvalue="glredbook10"/>
						<attr name="projectName" stringvalue="$example"/>
						<attr name="archive" stringvalue="glredbook10.jar"/>
						<attr name="srcPath" stringvalue=""/>
						<attr name="archiveExcludes" stringvalue=""/>
						<attr name="SystemFileSystem.localizingBundle" stringvalue="net.highteq.gamedev.nbm.joglglredbookproject.Bundle"/>
						<attr name="instantiatingIterator" methodvalue="net.highteq.gamedev.nbm.joglproject.ProjectWizardIterator.createIterator"/>
						<attr name="instantiatingWizardURL" urlvalue="nbresloc:/net/highteq/gamedev/nbm/joglglredbookproject/resources/${example}.html"/>
						<attr name="template" boolvalue="true"/>
					</file>
					#end
					
				</folder>
			</folder>
		</folder>
	</folder>
</filesystem>


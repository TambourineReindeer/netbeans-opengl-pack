<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.apisupport.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/nb-module-project/3">
            <code-name-base>javax.media.opengl</code-name-base>
            <suite-component/>
            <class-path-extension>
                <runtime-relative-path>ext/jogl.jar</runtime-relative-path>
                <binary-origin>release/modules/ext/jogl.jar</binary-origin>
            </class-path-extension>
            <module-dependencies>
                <dependency>
                    <code-name-base>com.sun.gluegen</code-name-base>
                    <run-dependency>
                        <specification-version>1.0.6</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>net.java.nativelibsupport</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>0.5</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.filesystems</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>7.3.1</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.modules</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>7.3.1</specification-version>
                    </run-dependency>
                </dependency>
                <dependency>
                    <code-name-base>org.openide.util</code-name-base>
                    <build-prerequisite/>
                    <compile-dependency/>
                    <run-dependency>
                        <specification-version>7.10.1.1</specification-version>
                    </run-dependency>
                </dependency>
            </module-dependencies>
            <public-packages>
                <package>com.sun.opengl.cg</package>
                <package>com.sun.opengl.util</package>
                <package>com.sun.opengl.util.j2d</package>
                <package>com.sun.opengl.util.texture</package>
                <package>com.sun.opengl.util.texture.spi</package>
                <package>javax.media.opengl</package>
                <package>javax.media.opengl.glu</package>
                <package>net.java.nboglpack.jogl.util</package>
            </public-packages>
        </data>
    </configuration>
</project>

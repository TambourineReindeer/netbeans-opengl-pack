    <volume>
        <type>src</type>
        <resource>jar:nbinst:///jogl-runtime/jogl-src.zip!/jogl/src/classes/</resource>
        <resource>jar:nbinst:///jogl-runtime/jogl-src.zip!/jogl/build/gensrc/classes/</resource>
    </volume>
